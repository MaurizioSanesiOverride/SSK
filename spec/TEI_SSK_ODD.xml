        <body>
            <p>My TEI Customization&#xD; starts with modules tei, core, textstructure and header</p>
            <schemaSpec ident="TEI_SSK" docLang="en" prefix="tei_" xml:lang="en">
                <moduleRef key="core" except=""/>
                <moduleRef key="tei" except=""/>
                <moduleRef key="header" except=""/>
                <moduleRef key="textstructure" except=""/>
                <moduleRef key="namesdates" except=""/>
                <moduleRef key="linking" except=""/>
                <elementSpec ident="div" mode="change">
                    <attList>
                        <attDef ident="type" usage="req" mode="change">
                            <gloss>You must indicate the level of the scenario</gloss>
                            <datatype>
                                <dataRef key="teidata.enumerated"/>
                            </datatype>
                            <valList type="closed" mode="replace">
                                <valItem ident="description">
                                    <gloss>Scenario description</gloss>
                                </valItem>
                                <valItem ident="subscenario">
                                    <gloss xml:lang="en">Sub Scenarios</gloss>
                                    <gloss xml:lang="fr"/>
                                    <desc>one or more steps in a research process, that can be
                                        combined to create a complete research scenario</desc>
                                </valItem>
                                <valItem ident="scenario">
                                    <gloss xml:lang="en">Research scenario</gloss>
                                    <gloss xml:lang="fr">Scénarion de recherche</gloss>
                                    <desc> combination of sub scenarios that represent a complet
                                        research process, from the definition of the research
                                        question to the publishing of the results</desc>
                                </valItem>
                            </valList>
                        </attDef>
                    </attList>
                </elementSpec>
                <elementSpec xmlns="http://www.tei-c.org/ns/1.0" module="namesdates" ident="event"
                    mode="replace">
                    <gloss versionDate="2008-12-09" xml:lang="fr">évènement</gloss>
                    <desc versionDate="2017-05-30" xml:lang="en">The <gi>event</gi> is the core
                        element of the SSK scneario. It is usable in two different contexts: <list>
                            <item>A full description of the scenario step, with all the
                                bibliographical references included. This events are stored in
                                single files.</item>
                            <item>In a given scenario, <gi>event</gi> elements are used as pointers
                                to link to full <gi>event</gi> elements stored in external
                                files</item>
                        </list>
                    </desc>
                    <classes>
                        <memberOf key="att.global"/>
                        <memberOf key="att.datable"/>
                        <memberOf key="att.editLike"/>
                        <memberOf key="att.typed"/>
                        <memberOf key="att.naming"/>
                        <memberOf key="att.sortable"/>
                        <memberOf key="model.eventLike"/>
                    </classes>
                    <content>
                        <sequence>
                            <classRef key="model.headLike" minOccurs="0" maxOccurs="unbounded"/>
                            <alternate>
                                <classRef key="model.pLike" minOccurs="0" maxOccurs="unbounded"/>
                                <classRef key="model.labelLike" minOccurs="0" maxOccurs="unbounded"
                                />
                            </alternate>
                            <alternate minOccurs="0" maxOccurs="unbounded">
                                <classRef key="model.noteLike"/>
                                <classRef key="model.biblLike"/>
                                <elementRef key="linkGrp"/>
                                <elementRef key="link"/>
                            </alternate>
                            <elementRef key="event" minOccurs="0" maxOccurs="unbounded"/>
                        </sequence>
                    </content>
                    <attList>
                        <attDef ident="type" usage="req" mode="change">
                            <datatype>
                                <dataRef key="teidata.enumerated"/>
                            </datatype>
                            <valList type="closed" mode="replace">
                                <valItem ident="step"/>
                            </valList>
                        </attDef>
                        <attDef ident="mode" usage="opt" mode="add">
                            <desc versionDate="2010-05-11" xml:lang="en">specifies the effect of
                                this declaration on its parent object.</desc>
                            <datatype>
                                <dataRef key="teidata.enumerated"/>
                            </datatype>
                            <defaultVal>change</defaultVal>
                            <valList type="closed">
                                <valItem ident="change">
                                    <desc versionDate="2010-05-11" xml:lang="en">this declaration
                                        changes the declaration of the same name in the current
                                        definition</desc>
                                    <desc versionDate="2007-12-20" xml:lang="ko">이 선언은 현 정의에서 동일 이름의
                                        선언을 변경한다.</desc>
                                    <desc versionDate="2007-05-02" xml:lang="zh-TW"
                                        >該宣告改變在現有定義中名稱相同的宣告。</desc>
                                    <desc versionDate="2008-04-06" xml:lang="es">esta declaración
                                        cambia la declaración del mismo nombre en la definición
                                        actual</desc>
                                    <desc versionDate="2008-04-05" xml:lang="ja"
                                        >当該宣言は，現行定義中の同名宣言を修正する．</desc>
                                    <desc versionDate="2008-03-30" xml:lang="fr">cette déclaration
                                        modifie la déclaration de même nom dans la définition
                                        courante.</desc>
                                    <desc versionDate="2007-01-21" xml:lang="it">la dichiarazione
                                        modifica la dichiarazione con lo stesso nome nella
                                        definizione corrente</desc>
                                </valItem>
                            </valList>
                        </attDef>
                        <attDef ident="where" usage="opt">
                            <desc versionDate="2007-06-11" xml:lang="en">indicates the location of
                                an event by pointing to a <gi>place</gi> element</desc>
                            <desc versionDate="2007-12-20" xml:lang="ko"><gi>place</gi> 요소를 지시함으로써
                                사건의 위치를 나타낸다.</desc>
                            <desc versionDate="2008-04-06" xml:lang="es">indica la localización de
                                un acontecimiento señalando un elemento <gi>lugar</gi>
                            </desc>
                            <desc versionDate="2008-12-09" xml:lang="fr">indique la localisation
                                d'un évènement en pointant vers un élément <gi>place</gi>
                            </desc>
                            <desc versionDate="2007-11-06" xml:lang="it">indica la posizione di un
                                evento facendo riferimento a un elemento place</desc>
                            <datatype>
                                <dataRef key="teidata.pointer"/>
                            </datatype>
                        </attDef>
                    </attList>
                    <exemplum xml:lang="en">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <person>
                                <event type="mat" when="1972-10-12">
                                    <label>matriculation</label>
                                </event>
                                <event type="grad" when="1975-06-23">
                                    <label>graduation</label>
                                </event>
                            </person>
                        </egXML>
                    </exemplum>
                    <exemplum versionDate="2008-04-06" xml:lang="fr">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <person>
                                <event type="mat" when="1972-10-12">
                                    <label>inscription</label>
                                </event>
                                <event type="grad" when="1975-06-23">
                                    <label>diplômé</label>
                                </event>
                            </person>
                        </egXML>
                    </exemplum>
                    <exemplum xml:lang="zh-TW">
                        <egXML xmlns="http://www.tei-c.org/ns/Examples">
                            <person>
                                <event type="mat" when="1972-10-12">
                                    <label>入學</label>
                                </event>
                                <event type="grad" when="1975-06-23">
                                    <label>畢業</label>
                                </event>
                            </person>
                        </egXML>
                    </exemplum>
                    <listRef>
                        <ptr target="#NDPERSbp"/>
                    </listRef>
                </elementSpec>
